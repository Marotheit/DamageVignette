{
  "schemaVersion": 1,
  "id": "healthvignette",
  "version": "${mod_version}",
  "name": "HealthVignette",
  "description": "Displays a vignette effect that increases with intensity as player health decreases.",
  "authors": [
    "Octol1ttle",
    "Marotheit"
  ],
  "contact": {
    "source": "https://github.com/Marotheit/HealthVignette"
  },
  "license": "MIT",
  "icon": "assets/healthvignette/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.sanctuaryhosting.healthvignette.HealthVignette"
    ],
    "modmenu": [
      "net.sanctuaryhosting.healthvignette.ModMenu"
    ]
  },
  "mixins": [
    "healthvignette.mixins.json"
  ],
  "depends": {
    "minecraft": "~${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
